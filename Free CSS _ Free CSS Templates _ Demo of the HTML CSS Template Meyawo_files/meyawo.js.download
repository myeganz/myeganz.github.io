$(document).ready(function(){$(".navbar .nav-link").on("click",function(n){if(""!==this.hash){n.preventDefault();var t=this.hash;$("html, body").animate({scrollTop:$(t).offset().top},700,function(){window.location.hash=t})}})}),$("#nav-toggle").click(function(){$(this).toggleClass("is-active"),$("ul.nav").toggleClass("show")});